  "version": "0.2.0",
  "configurations": [
  {
    "name": "Local GO debug",
    "type": "go",
    "request": "launch",
    "mode": "debug",
    "program": "${workspaceFolder}/mockservice/main.go"
  },
  {
    "name": "Remote Docker",
    "type": "go",
    "request": "launch",
    "mode": "remote",
    "remotePath": "/go/src/app/mockservice",
    "port": 2345,
    "host": "127.0.0.1",
    "program": "${workspaceFolder}/mockservice",
    // "program": "${workspaceFolder}/forex-dashboard/mockservice",
    "args": [],
    "trace": "verbose",
    "env": {}
  },
  {
    "name": "Launch",
